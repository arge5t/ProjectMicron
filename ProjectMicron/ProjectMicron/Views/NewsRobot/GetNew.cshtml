
@model ProjectMicron.Domain.ViewModels.New.NewsRobotViewModel
@{
    ViewData["Title"] = "GetNew";
}




<section class="news">
    <div class="container">

        <div class="news__inner get-new">

            <a class="news__link">
                <img src="data:image/jpeg;base64," alt="" class="news__images">
            </a>

            <p class="news__deta">Последние изменения - @Model?.CreationTime</p>
            <div class="news__info">

                <h2 class="news__title">@Model?.Title </h2>

                <p class="news__text get-new__text" style="width:auto;">
                    @Model?.Description
                </p>

            </div>
        </div>

      

    </div>
</section>